{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "python.linting.enabled": false,
  "cSpell.words": [
    "dbscan"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}
