{
  "name": "wwa",
  "version": "1.0.0",
  "description": "World Water Atlas",
  "author": "Fedor Baart <fedor.baart@deltares.nl>",
  "private": true,
  "scripts": {
    "dev": "npm-run-all build dev:app",
    "dev:app": "nuxt dev",
    "build": "npm-run-all build:*",
    "build:data": "dato dump",
    "build:assets": "cpy ./components/**/assets/*.* ./static/assets",
    "generate": "nuxt build && nuxt export --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "data:backup": "dato dump --config dato.backup.js",
    "data:app": "cross-env DATO_EXPORT=app dato dump",
    "data:books": "cross-env DATO_EXPORT=books dato dump",
    "data:chapters": "cross-env DATO_EXPORT=chapters dato dump",
    "data:keywords": "cross-env DATO_EXPORT=keywords dato dump",
    "data:influences": "cross-env DATO_EXPORT=influences dato dump",
    "data:static-pages": "cross-env DATO_EXPORT=static_pages dato dump",
    "data:themes": "cross-env DATO_EXPORT=themes dato dump",
    "data:goals": "cross-env DATO_EXPORT=goals dato dump",
    "data:methodologies": "cross-env DATO_EXPORT=methodologies dato dump",
    "data:globe-data": "cross-env DATO_EXPORT=globe_data dato dump"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "d3-array": "^2.4.0",
    "d3-collection": "^1.0.7",
    "d3-jetpack": "^2.0.20",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "dbscanjs": "^1.0.1",
    "es6-tween": "^5.5.11",
    "iso-639-1": "^2.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "mapbox-gl": "^1.11.0",
    "marked": "^1.1.0",
    "nuxt": "^2.13.1",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^5.4.5",
    "three": "^0.116.1",
    "three.meshline": "^1.2.0",
    "typeface-roboto": "0.0.75",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "bent": "^7.3.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "datocms-client": "^0.3.41",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.30",
    "postcss-calc": "^7.0.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "slug": "^2.1.1",
    "webpack-glsl-loader": "^1.0.1"
  }
}
